@page "{id:int}"
@model QuantumEncryption.Pages.RSAKeyModel
@{
    ViewData["Title"] = "RSAKey";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<partial name="_header" model="Model.SessionModel" />
<div class="container">
    <main role="main" class="pb-3">
           <div class="row">

                <div class="col-md-12">
                    <div class="panel panel-primary">
                        <div class="panel-heading">Quantum Key</div>
                        <div class="panel-body">
                            <div class="form-group">
                                @if (!string.IsNullOrEmpty(Model.SessionModel.Binary))
                                {
                                    <label for="exampleInputEmail1">@Model.SessionModel.Binary </label>
                                }
                                @if (!string.IsNullOrEmpty(Model.SessionModel.Decimal))
                                {
                                    <label for="exampleInputPassword1">@Model.SessionModel.Decimal </label>
                                }

                            </div>
                            <div class="form-group">
                                <label for="exampleInputPassword1">Q-Key Calculation Speed:</label>
                                
                            </div>
                            <div class="form-group">
                                <label for="exampleInputPassword1">Q-Key Calculation Time:</label>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="col-md-12">
                    <div class="panel panel-primary">
                        <div class="panel-heading">RSA Key</div>
                        <div class="panel-body">
                            <div class="form-group">
                                <label for="exampleInputEmail1">P : @Model.SessionModel.P</label> <label for="exampleInputPassword1">Q : @Model.SessionModel.Q</label>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputEmail1">Public Key: @Model.SessionModel.UserPublicKey</label>
                            </div>   
                            <div class="form-group">
                                <label for="exampleInputEmail1">Private Key: @Model.SessionModel.UserPrivateKey</label>
                            </div>
                        </div>
                    </div>

                </div>

            </div>

    </main>
</div>

